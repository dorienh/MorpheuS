    public enum RootNote {
        C, D, E, F, G, A, B, Cs, Cf, Ds, Df, Es, Ef, Fs, Ff, Gs, Gf, As, Af, Bs, Bf;

        public static RootNote getRandom() {
            Random random = new Random();
            return values()[random.nextInt(values().length)];
        }
    }

